// Config file for Didymos Optimization Project
// Randomization seed, set to NONE for time(0) as seed
time_seed=NONE

// Initial start point for the algorithm, initial_start_file_address only used if random_start is set to "false"
random_start=true
initial_start_file_address=../OptimizedVector.bin

// Output frequency variables
write_freq=1
disp_freq=10

// Algorithm Solution demands
best_count=10
pos_threshold=1.0e-10
speed_threshold=4.4e-8
coast_threshold=0.5
change_check=100
coast_threshold=0.5

// Anneal determination properties
anneal_factor=0.5
anneal_initial=0.001

// Mutation Rates and scale for specific parameter types
mutation_rate=0.15
double_mutate_rate=0.2
triple_mutate_rate=0.05
gamma_mutate_scale=10.0
tau_mutate_scale=10.0
coast_mutate_scale=10.0
triptime_mutate_scale=0.5
zeta_mutate_scale=1.57
beta_mutate_scale=3.14
alpha_mutate_scale=3.14

// Thruster: 0 is none and 1 is NEXT ion thruster
thruster_type=0

// Mass of the spacecraft and fuel
dry_mass=2700
// fuel_mass
wet_mass=3000

// Determines initial velocity magnitude
// Mission is 4.676e6
c3energy=4.676e6

// Asteriod position and velocity at time of impact
// Values based on date: ##-##-#### (Note on June 23 2020: Values need to be updated and date here added)
r_fin_ast=1.02696822710421
theta_fin_ast=0.238839574416454
z_fin_ast=-0.0526614832914496

vr_fin_ast=-2.05295246185041e-08
vtheta_fin_ast=2.29132593453064e-07
vz_fin_ast=8.00663905822009e-09

// // Earth position and velocity at time of impact
r_fin_earth=1.00021392223428
theta_fin_earth=0.199470650149394
z_fin_earth=-1.54878511585620e-05

vr_fin_earth=-3.32034068725821e-09
vtheta_fin_earth=1.99029138292504e-07
vz_fin_earth=-9.71518257891386e-12